{
    "name": "finally-a-fast/faftephp",
    "keywords": [
        "faftephp"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "homepage": "https://www.finally-a-fast.com/",
    "support": {
        "issues": "https://gitlab.com/finally-a-fast/faftephp/issues",
        "source": "https://gitlab.com/finally-a-fast/faftephp",
        "docs": "https://www.finally-a-fast.com/packages/faftephp/docs"
    },
    "authors": [
        {
            "name": "Christoph MÃ¶ke",
            "email": "christophmoeke@gmail.com",
            "role": "Project Leader"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-intl": "*",
        "jlawrence/eos": "^3.2",
        "psr/log": "^1.1",
        "psr/simple-cache": "^1.0",
        "yiisoft/validator": "^3.0",
        "ivopetkov/html5-dom-document-php": "^2.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "yiisoft/cache": "^3.0",
        "yiisoft/cache-apcu": "^3.0",
        "yiisoft/log": "^3.0",
        "yiisoft/log-target-file": "^3.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "build": [
        ]
    },
    "autoload": {
        "psr-4": {
            "fafte\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
